item()
  width calc(100% - 50px)
  margin-right 50px

option()
  overflow hidden
  white-space nowrap
  text-overflow ellipsis
  width 100%
  height 90px
  box-sizing border-box
  padding 5px 30px
  font-size 40px
  font-weight bold
  text-align center
  color var(--text-color)
  background var(--option-background-color)
  border 2px solid var(--border-hover-color)
  box-shadow 2px 2px 10px var(--border-hover-color)

bubbles-0()
  .text-box
    .text
      background #d3bb8b !important
      border-radius 25px 0 25px 25px !important
      box-shadow 2px 4px #9d9f9f !important

$bubbles-1 = url('data:image/svg+xml;base64,')

bubbles-1()
  .text-box
    margin 30px 40px
    border 1px solid transparent
    border-image-source $bubbles-1
    border-image-slice 70 85 45 110 fill
    border-image-width 70px 85px 45px 110px
    border-image-outset 35px 60px 25px 65px
    border-image-repeat stretch

    .text
      background none !important
      border-radius 0 !important
      box-shadow none !important
      padding 5px !important
      margin 30px 0 15px 15px !important
      color #fffafb !important
